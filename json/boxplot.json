{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": {
    "text": "Distribution of Happiness Score by Region",
    "padding": 5,
    "fontSize": 17,
    "fontWeight": 600
  },
  "description": "A vertical box plot showing median and lower and upper quartiles of the distribution of Happiness Score of regions.",
  "width": "container",
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/GovindChadha/3179Assignment2/main/Dataset/result.csv"
  },
  "mark": "boxplot",
  "encoding": {
    "x": {
      "field": "Region",
      "type": "nominal",
      "axis": {"labelAlign": "left", "labelAngle": 30}
    },
    "color": {"value": "purple"},
    "y": {
      "field": "Happiness Score",
      "type": "quantitative",
      "scale": {"zero": false}
    },
    "tooltip": [
      {"field": "Country", "type": "nominal"},
      {"field": "Region", "type": "nominal"},
      {"field": "Happiness Score", "type": "quantitative"}
    ]
  },
  "background": "white",
  "config": {}
}